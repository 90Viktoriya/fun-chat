{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "import",
    "prettier"
  ],
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base",
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "parserOptions": {
    "sourceType": "module",
    "project": ["**/tsconfig.json"]
  },
  "rules": {
    "no-console": 0,
    "@typescript-eslint/no-explicit-any": "error",
    "curly": ["error", "all"],
    "prettier/prettier": 2
},
"settings": {
  "import/resolver": {
    "typescript": true
  },
  "import/parsers": {
    "@typescript-eslint/parser": [".ts"]
  }
},
"noInlineConfig": true
}